{
  "name": "viewar-react-boilerplate",
  "version": "1.0.1",
  "description": "React boilerplate app for ViewAR SDK",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf build && rimraf bundle",
    "deploy": "viewar-cli deploy",
    "build": "npm run clean && webpack -p --config ./config/webpack.config.js --mode production --env production",
    "start": "npm run clean && webpack-dev-server --config ./config/webpack.config.js --open --mode development --env development_core",
    "start:mock": "npm run clean && webpack-dev-server --config ./config/webpack.config.js --open --mode development --env development_mock",
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "prettier --config ./.prettierrc './**/*.{css,scss,md,json}' --write && prettier-eslint --config ./.prettierrc --write \"**/*.*{js,jsx}\""
  },
  "author": "Karl Hofer <kh@viewar.com>",
  "license": "MIT",
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "modules": "commonjs",
          "node": true,
          "comments": true,
          "targets": {
            "esmodules": true,
            "browsers": [
              "> 1%",
              "last 2 versions",
              "not ie <= 8"
            ]
          }
        }
      ],
      "@babel/preset-react"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.5.5",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.4",
    "babel-polyfill": "^6.26.0",
    "body-parser": "^1.19.0",
    "chalk": "^2.4.2",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^2.1.0",
    "eslint": "^6.2.0",
    "eslint-config-standard": "^13.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx": "^0.1.0",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-standard": "^4.0.1",
    "file-loader": "^3.0.1",
    "glob": "^7.1.4",
    "html-webpack-plugin": "^3.2.0",
    "indent-string": "^4.0.0",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "postcss": "^7.0.7",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.5.0",
    "prettier": "^1.15.3",
    "prettier-eslint": "^9.0.0",
    "prettier-eslint-cli": "^5.0.0",
    "react-hot-loader": "^4.3.12",
    "rimraf": "^2.6.3",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.28.3",
    "webpack-cli": "^3.2.0",
    "webpack-dev-server": "^3.1.14",
    "webpack-merge": "^4.1.5"
  },
  "dependencies": {
    "fastclick": "^1.0.6",
    "normalize.css": "^8.0.1",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "viewar-api": "^0.47.1",
    "viewar-core": "^11.34.2"
  }
}
